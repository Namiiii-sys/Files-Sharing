generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model File {
  id        String   @id @default(cuid())
  name      String
  url       String
  type      String
  size      Int
  password  String?
  shortId   String   @unique
  createdAt DateTime @default(now())
  userId    String 
}
